<Window x:Class="_6040CEM_GUI_Windows.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_6040CEM_GUI_Windows"
        xmlns:view="clr-namespace:_6040CEM_GUI_Windows.View"
        mc:Ignorable="d"
        Title="6040CEM PIC18F4520 Sensor to GUI" Height="450" Width="800">
    <Grid>
        <Grid>
            <Frame x:Name="mainFrame" Grid.Row="0" />

            <TabControl>
                <TabItem Header="Main">
                    <Frame Source="/View/FrontPage.xaml" />
                </TabItem>
                <TabItem Header="Settings">
                    <Frame Source="/View/SettingsPage.xaml" />
                </TabItem>
                <TabItem Header="Graphs">
                    <Frame Source="/View/GraphsPage.xaml" />
                </TabItem>
            </TabControl>

        </Grid>
    </Grid>
</Window>
